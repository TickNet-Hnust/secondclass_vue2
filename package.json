{
    "name": "secondclass",
    "version": "3.5.0",
    "description": "第二课堂管理端",
    "author": "若依",
    "license": "MIT",
    "scripts": {
        "dev": "vue-cli-service serve",
        "build:prod": "vue-cli-service build",
        "build:stage": "vue-cli-service build --mode staging",
        "deploy:prod": "deploy-cli-service deploy --mode prod",
        "deploy:test": "deploy-cli-service deploy --mode test",
        "preview": "node build/index.js --preview",
        "lint": "eslint --ext .js,.vue src",
        "prettier": "prettier ** --write --ignore-unknown",
        "doc": "jsdoc -r -c /jsdoc.conf.json"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*.{js,vue}": [
            "eslint --fix",
            "git add"
        ]
    },
    "keywords": [
        "vue",
        "admin",
        "dashboard",
        "element-ui",
        "boilerplate",
        "admin-template",
        "management-system"
    ],
    "repository": {
        "type": "git",
        "url": "https://https://gitlab.com/mingyuefusu/second_class_front"
    },
    "dependencies": {
        "@riophae/vue-treeselect": "0.4.0",
        "ant-design-vue": "1.7.8",
        "axios": "0.21.4",
        "clipboard": "2.0.6",
        "compression-webpack-plugin": "5.0.2",
        "compressorjs": "1.1.1",
        "core-js": "3.8.1",
        "echarts": "4.9.0",
        "element-ui": "2.15.6",
        "file-saver": "2.0.4",
        "fuse.js": "6.4.3",
        "highlight.js": "9.18.5",
        "horwheel": "1.0.1",
        "js-beautify": "1.13.0",
        "js-cookie": "2.2.1",
        "jsencrypt": "3.0.0-rc.1",
        "less": "4.1.2",
        "less-loader": "2.2.3",
        "nprogress": "0.2.0",
        "quill": "1.3.7",
        "screenfull": "5.0.2",
        "sortablejs": "1.10.2",
        "v-viewer": "1.6.4",
        "vue": "2.6.14",
        "vue-count-to": "1.0.13",
        "vue-cropper": "0.5.8",
        "vue-router": "3.4.9",
        "vuedraggable": "2.24.3",
        "vuex": "3.6.0",
        "wangeditor": "4.7.11",
        "x-scrollbar": "3.1.0"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "4.4.6",
        "@vue/cli-plugin-eslint": "4.4.6",
        "@vue/cli-service": "4.4.6",
        "babel-eslint": "10.1.0",
        "chalk": "4.1.2",
        "connect": "3.6.6",
        "eslint": "7.15.0",
        "eslint-plugin-vue": "7.2.0",
        "lint-staged": "10.5.3",
        "mockjs": "1.1.0",
        "runjs": "4.4.2",
        "sass": "1.32.0",
        "sass-loader": "10.2.1",
        "script-ext-html-webpack-plugin": "2.1.5",
        "svg-sprite-loader": "5.1.1",
        "vue-template-compiler": "2.6.14"
    },
    "engines": {
        "node": ">=8.9",
        "npm": ">= 3.0.0"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ]
}
